-- 7375

-- iterative fibonacci
fn ifib(n: u16) -> u64:
  a: u64 = 0
  b: u64 = 1

  for .., n > 0, n -= 1:
    t: u64 = a

    a = b
    b = t + b

  return a

-- recursive fibonacci
fn rfib(n: u16) -> u64:
  return n as u64 if n <= 1 else rfib(n - 1) + rfib(n - 2)

fn main(argc: u32, argv: **u8) -> i32:
  return (ifib(20) + rfib(15)) as i32
