-- 1100

-- iterative pow
fn ipow(base: u64, exp: u16) -> u64:
  r: u64 = 1

  for .., exp > 0, exp -= 1:
    r *= base

  return r

-- recursive pow
fn rpow(base: u64, exp: u16) -> u64:
  return 1 if exp == 0 else base * rpow(base, exp - 1)

fn main(argc: u32, argv: **u8) -> i32:
  return (ipow(10, 2) + rpow(10, 3)) as i32
