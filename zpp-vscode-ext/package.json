{
  "name": "zpp",
  "displayName": "zpp",
  "description": "zpp syntax support",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "zpp",
      "aliases": ["zpp", "zpp"],
      "extensions": [".zpp"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "zpp",
      "scopeName": "source.zpp",
      "path": "./syntaxes/zpp.tmLanguage.json"
    }]
  }
}
